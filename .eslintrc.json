{
  "extends": "airbnb",
  "env": {
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-debugger": "off",
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  },
  "settings": {
    "import/resolver": "babel-plugin-root-import"
  }
}
